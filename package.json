{
  "name": "@openfn/language-twilio",
  "version": "0.2.0",
  "description": "An Language Package for twilio",
  "main": "lib/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src -d lib && npm run ast",
    "test": "mocha --compilers js:babel-core/register",
    "test:watch": "mocha -w --compilers js:babel-core/register",
    "ast": "simple-ast --adaptor ./src/Adaptor.js --output ast.json",
    "postversion": "git push && git push --tags",
    "version": "npm run build && git add -A lib ast.json"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "lib/",
    "ast.json"
  ],
  "dependencies": {
    "@openfn/language-common": "^1.4",
    "twilio": "^3.65.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.9.0",
    "@openfn/simple-ast": "^0.3",
    "assertion-error": "^1.0.1",
    "chai": "^3.4.0",
    "deep-eql": "^0.1.3",
    "jsdoc": "^3.6.6",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "sinon": "^1.17.2"
  },
  "directories": {
    "lib": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/language-twilio.git"
  },
  "bundledDependencies": [
    "language-common",
    "twilio"
  ]
}
